{
  "ConnectionStrings": {
    "DB_CONNECTION_STRING": "Data Source=wesafe.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "PhysicalFileStorage": {
    "Root": "/app/files",
    "RequestPath": "/files",
    "BaseUrl": ""
  },

  "CleanupLogs": {
    "KeepingDeviceLogsDays": 7,
    "KeepingCameraLogsDays": 30,
    "KeepingUnhandledExceptionsDays": 7
  }
}
